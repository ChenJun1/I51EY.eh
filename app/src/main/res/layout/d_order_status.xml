<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/AB_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/circle_grey"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/AB_v"
                    android:layout_width="1dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:background="#999999" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp" >

                <TextView
                    android:id="@+id/AB_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已接单"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/Received_Orders_Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/AC_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/circle_grey"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/AC_v"
                    android:layout_width="1dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:background="#999999" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp" >

                <TextView
                    android:id="@+id/AC_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已提货"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tihuoshijian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                     />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/TH_Photo"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:background="@drawable/setbar_bg4"
                        android:text="查看图片"
                        android:textColor="#8BC1E9"
                        android:textSize="12sp"
                        android:visibility="gone" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/AD_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/circle_grey"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/AD_v"
                    android:layout_width="1dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:background="#999999" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp" >

                <TextView
                    android:id="@+id/AD_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="运输中"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/AD_time_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                     />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/AF_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/circle_grey"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/AF_v"
                    android:layout_width="1dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:background="#999999" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp" >

                <TextView
                    android:id="@+id/AF_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已交货"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/Goods_Unload_Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                   />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/JH_Photo"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:background="@drawable/setbar_bg4"
                        android:text="查看图片"
                        android:textColor="#8BC1E9"
                        android:textSize="12sp"
                        android:visibility="gone" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/AG_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/circle_grey"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp" >

                <TextView
                    android:id="@+id/AG_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已完成"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/Completed_Orders_Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                     />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>